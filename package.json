{
  "name": "@railway/roundhouse",
  "version": "0.1.0",
  "description": "Wrapper for the Railway public API",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": false,
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "bun run src/index.ts",
    "lint": "biome check . --write",
    "codegen": "graphql-codegen",
    "prepublishOnly": "bun run build",
    "clean": "rm -rf dist"
  },
  "author": "Carlos Medim <kadumedim123@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadumedim/roundhouse.git"
  },
  "bugs": {
    "url": "https://github.com/kadumedim/roundhouse/issues"
  },
  "homepage": "https://github.com/kadumedim/roundhouse#readme",
  "keywords": [
    "railway",
    "roundhouse",
    "sdk",
    "api",
    "typescript",
    "graphql",
    "wrapper"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "graphql-request": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
